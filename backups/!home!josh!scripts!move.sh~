#!/bin/sh

for i in ~/mnt/Music/*/*/*/*.flac ; do mv "${i/.flac/.mp3}" ~/mnt/Music/mp3 ; done
for i in ~/mnt/Music/*/*/*.flac ; do mv "${i/.flac/.mp3}" ~/mnt/Music/mp3 ; done
for i in ~/mnt/Music/*/*.flac ; do mv "${i/.flac/.mp3}" ~/mnt/Music/mp3 ; done
for i in ~/mnt/Music/*.flac ; do mv "${i/.flac/.mp3}" ~/mnt/Music/mp3 ; done
