alignment       	top_right
background      	no
border_width        	0
default_color       	ffffff
double_buffer       	yes
draw_borders        	no
draw_graph_borders  	no
draw_outline        	no
draw_shades     	no
gap_x           	-2400
gap_y               0
no_buffers      	yes
own_window      	yes
own_window_argb_value   255
own_window_argb_visual  yes
own_window_hints    	undecorated,below,sticky,skip_taskbar,skip_pager
own_window_transparent  yes
own_window_type         desktop
stippled_borders    	0
update_interval     	0.2
use_xft         	yes
xftalpha                0.8

