#!/bin/bash

file=$(~/mnt/160503\ Customer\ Cancelled\ List\ edit.csv)
line=`cat $file | grep -v School,Postcode | awk '{print}' ORS=' '`

if [[ -z "$line" ]] ; then
echo "Completed"
exit 0
else
echo "Removing lines"
cat ~/mnt/160503\ Edubase\ Data\ for\ Josh.csv | grep -v $line > ~/mnt/160503\ Edubase\ Data\ for\ Josh\ rem.csv
fi

exit 0
