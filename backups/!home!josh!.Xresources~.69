Xft.dpi: 72
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

*.foreground:   #E5E7EA
*.background:   #2B2E37
*.cursorColor:  #1685A6

! black
*.color0:       #1e1e1e
*.color8:       #5f5a60
! red
*.color1:       #cf6a4c
*.color9:       #cf6a4c
! green
*.color2:       #8f9d6a
*.color10:      #8f9d6a
! yellow
*.color3:       #f9ee98
*.color11:      #f9ee98
! blue
*.color4:       #7587a6
*.color12:      #7587a6
! magenta
*.color5:       #9b859d
*.color13:      #9b859d
! cyan
*.color6:       #afc4db
*.color14:      #afc4db
! white
*.color7:       #a7a7a7
*.color15:      #ffffff


URxvt*perl-ext-common:      default,clipboard,url-select,keyboard-select,resize-font,matcher,bell-command
URxvt*url-select*launcher:  /usr/bin/exo-open
URxvt*url-select*underline: true
URxvt*keysym*M-u:           perl:url-select:select_next
URxvt*keysym*M-Escape:      perl:keyboard-select:activate
URxvt*keysym*M-s:           perl:keyboard-select:search
URxvt*resize-font*bigger:	C-Up
URxvt*resize-font*smaller:	C-Down
URxvt*bell-command: notify-send "Beep, Beep"; mpv ~/mnt/Music/Kathy\ McCarty\ -\ Rocket\ Ship\ \(Z3D\ Rocket\ Remix\)-g6lWjL_nO-c\~20150912-162404.opus

URxvt*scrollTtyOutput:		false
URxvt*scrollWithBuffer:		true
URxvt*scrollTtyKeypress:	true
URxvt*font:					xft:Ubuntu Mono derivative Powerline Nerd Font Plus Font Awesome Plus Font Linux Mono Windows Compatible:size=12:antialias=true
URxvt*boldfont:				xft:Ubuntu Mono derivative Powerline Nerd Font Plus Font Awesome Plus Font Linux Mono Windows Compattible:bold:size=12:antialias=true
URxvt*italicfont:			xft:Ubuntu Mono derivative Powerline Nerd Font Plus Font Awesome Plus Font Linux Mono Windows Compatible:italic:size=13:antialias=true
URxvt*letterSpace:			-1
URxvt*lineSpace:			3
URxvt*cursorBlink:			true
URxvt*scrollBar:			false
URxvt*urgentOnBell:			true
URxvt*visualBell:			false
URxvt*internalBorder:		7
URxvt*transparent:			false
URxvt*cutchars:				"()*,<>[]{}|:'"
URxvt*blurRadius:			4
urxvt*highlightSelection:	true

urxvt*colorMode:		on
urxvt*colorBDMode:		on
urxvt*colorULMode:		on
urxvt*underLine:		on
URxvt*depth:			32
URxvt*fading:			30

!URxvt*tabbed.tabbar-fg: 7
!URxvt*tabbed.tabbar-bg: 0

rofi*color-enabled: true
rofi*color-window: #2B2E37, #2B2E37, #1e2529
rofi*color-normal: #2B2E37, #c1c1c1, #2B2E37, #394249, #ffffff
rofi*color-active: #2B2E37, #80cbc4, #2B2E37, #394249, #80cbc4
rofi*color-urgent: #2B2E37, #ff1844, #2B2E37, #394249, #ff1844
