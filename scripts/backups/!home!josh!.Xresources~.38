URxvt*perl-ext-common:      default,clipboard,url-select,keyboard-select
URxvt*url-select.launcher:  firefox
URxvt*url-select.underline: true
URxvt*keysym.M-u:           perl:url-select:select_next
URxvt*keysym.M-Escape:      perl:keyboard-select:activate
URxvt*keysym.M-s:           perl:keyboard-select:search

URxvt*scrollTtyOutput:		false
URxvt*scrollWithBuffer:		true
URxvt*scrollTtyKeypress:	true
URxvt*font:					xft:Ubuntu Mono derivative Powerline:size=11:antialias=true
URxvt*boldfont:				xft:Ubuntu Mono derivative Powerline:size=12:antialias=true
!URxvt*fontface:				6x15
!URxvt*letterSpace:			-2
URxvt*lineSpace:			3
URxvt*cursorBlink:			false
URxvt*scrollBar:			false
URxvt*urgentOnBell:			true
URxvt*visualBell:			false
URxvt*internalBorder:		7
URxvt*transparent:			false
URxvt*cutchars:				"()*,<>[]{}|:'"
URxvt*blurRadius:			4
urxvt*highlightSelection:	true

urxvt*colorMode:     on
urxvt*colorBDMode:   on
urxvt*colorULMode:   on
urxvt*underLine:     on
URxvt*depth: 32

!URxvt*tabbed.tabbar-fg: 7
!URxvt*tabbed.tabbar-bg: 0

rofi*color-enabled: true
rofi*color-window: #2B2E37, #2B2E37, #1e2529
rofi*color-normal: #2B2E37, #c1c1c1, #2B2E37, #394249, #ffffff
rofi*color-active: #2B2E37, #80cbc4, #2B2E37, #394249, #80cbc4
rofi*color-urgent: #2B2E37, #ff1844, #2B2E37, #394249, #ff1844
