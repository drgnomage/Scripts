Xft.dpi: 72
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

*.foreground:   #E5E7EA
*.background:   #2B2E37
*.cursorColor:  #24A5C3

!*color0: #161820
!*color1: #947ca0
!*color2: #298ca0
!*color3: #597ba0
!*color4: #9a6da0
!*color5: #6f5ea0
!*color6: #5ca097
!*color7: #a08297
!*color8: #7da5aa
!*color9: #c87498
!*color10: #e0858e
!*color11: #c8b3c4
!*color12: #e09b99
!*color13: #e0baba
!*color14: #e0ddca
!*color15: #e0ddd0

! black
*.color0:       #1e1e1e
*.color8:       #5f5a60
! red
*.color1:       #cf6a4c
*.color9:       #cf6a4c
! green
*.color2:       #8f9d6a
*.color10:      #8f9d6a
! yellow
*.color3:       #f9ee98
*.color11:      #f9ee98
