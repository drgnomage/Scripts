def isGettable(letter):
    if(letter=='C' or letter=='S' or letter=='R' or letter=='E' or letter=='B'):
        return 1
    if(letter=='U'):
        return -1
    return 0

def combine(array):

    
    
    combined=0
    unavailable=1
    for v in array:

        #calculate
        var=isGettable(csv[v])
        
        if(var==1):
            combined=1;
        if(var!=-1):
            unavailable=0;

    if(unavailable==1):
        return -1;
    else:
        return combined;
        




