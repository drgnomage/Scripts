own_window yes
own_window_transparent yes
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
own_window_argb_visual no
own_window_class Conky
own_window_title Conky
own_window_type override
out_to_console no
use_xft yes
update_interval 1
cpu_avg_samples 2
net_avg_samples 2
double_buffer yes
maximum_width 600
minimum_size 120 900
draw_shades no
draw_outline no
draw_borders no
stippled_borders 0
border_width 20
default_color 000000
default_shade_color 000000
default_outline_color 000000
alignment top_left
gap_x 1322
gap_y 20
use_spacer left
no_buffers yes
uppercase no
background yes
own_window_argb_value 255

font Source Code Pro for Powerline:size=10:antialias=true

color1 8f9d6a
color2 202020
color3 7587a6
color4 2B2E37
color5 1e1e1e

