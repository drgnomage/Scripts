backend = "glx";
shadow = true;
no-dock-shadow = true;
no-dnd-shadow = true;
clear-shadow = true;
shadow-radius = 3;
shadow-offset-x = -8;
shadow-offset-y = -2;
shadow-exclude = "i:e:0x5200001";
shadow-ignore-shaped = true;
menu-opacity = 1.0;
inactive-opacity = 0.9;
inactive-opacity-override = true;
blur-background = true;
blur-background-exclude = [ "window_type = 'dock'", "window_type = 'desktop'", "_GTK_FRAME_EXTENTS@:c" ];
fading = true;
fade-in-step = 0.15;
fade-out-step = 0.1;
mark-wmwin-focused = true;
mark-ovredir-focused = true;
detect-rounded-corners = true;
wintypes : 
{
  tooltip : 
  {
    fade = true;
    shadow = false;
    opacity = 0.75;
  };
};
shadow-red = 0.16078431372549;
shadow-green = 0.203921568627451;
shadow-blue = 0.227450980392157;
blur-background-frame = true;
no-fading-openclose = false;
fade-delta = 20;
inactive-dim = 0.1;
alpha-step = 0.1;
frame-opacity = 0.9;
inactive-dim-fixed = false;
